<h1>Events</h1>
<div class="">
    <% @events.each do |event| %>
        <div class="event">
            <div class="title"><%= event.title %></div>
            <div class="creator"><%= event.creator.name %></div>
            <div class="description"><%= event.description %></div>
            <div class="datetime"><%= event.date_time %></div>
        </div>
    <% end %>
</div>