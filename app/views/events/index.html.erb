<h1>Events</h1>
<div class="">
    <% @events.each do |event| %>
        <div class="event">
            <div class="title"><%= event.title %></div>
            <div class="creator">creator</div>
            <div class="description"><%= event.description %></div>
            <div class="datetime"><%= event.datetime %></div>
        </div>
    <% end %>
</div>